{
  "manifest_version": 3,
  "name": "idb crud - (IndexedDB Manager)",
  "description": "idb crud enhance your indexedDb management",
  "version": "0.2",
  "action": {
    "default_icon": {
      "16": "media/appicon16.png",
      "32": "media/appicon32.png",
      "48": "media/appicon48.png",
      "128": "media/appicon128.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "js": [
        "src/content.jsx"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "src/background.js"
  }
}
